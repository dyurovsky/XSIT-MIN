journal = {Neural networks},
keywords = {Algorithms,Animals,Bayes Theorem,Biological,Brain,Brain: cytology,Brain: physiology,Computer-Assisted,Electromyography,Haplorhini,Least-Squares Analysis,Linear Models,Models,Neural Networks (Computer),Neurons,Neurons: physiology,Signal Processing},
pages = {1112--1131},
