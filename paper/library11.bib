journal = {Nature},
keywords = {Achievement,Adolescent,Child,Cognition,Cognition: physiology,Education,Evolution,Female,Humans,Individuality,Linear Models,Longitudinal Studies,Male,Mathematics,Preschool,Schools},
pages = {665--668},
pmid = {18776888},
title = {{Individual differences in non-verbal number acuity correlate with maths achievement.}},
